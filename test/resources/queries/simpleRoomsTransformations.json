{
	"title": "Success: General rooms dataset query, validation of TRANSFORMATIONS",
	"input":
	{
		"WHERE": {
			"AND": [
				{
					"IS": {
						"rooms_furniture": "*Tables*"
					}
				},
				{
					"GT": {
						"rooms_seats": 300
					}
				}
			]
		},
		"OPTIONS": {
			"COLUMNS": [
				"rooms_shortname",
				"maxSeats"
			],
			"ORDER": {
				"dir": "DOWN",
				"keys": ["maxSeats"]
			}
		},
		"TRANSFORMATIONS": {
			"GROUP": ["rooms_shortname"],
			"APPLY": [{
				"maxSeats": {
					"MAX": "rooms_seats"
				}
			}]
		}
	},
	"with": [
		{
			"rooms_shortName": "OSBO",
			"maxSeats": 442
		},
		{
			"rooms_shortName": "HEBB",
			"maxSeats": 375
		},
		{
			"rooms_shortName": "LSC",
			"maxSeats": 350
		}
	]
}
